{
  "name": "i18n-auto-translation",
  "version": "1.2.4",
  "description": "Auto translate i18n JSON file(s) to desired language(s).",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://github.com/while1618/i18n-auto-translation",
  "bin": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check-updates": "npx npm-check-updates"
  },
  "keywords": [
    "i18n",
    "auto translate",
    "translation",
    "google translate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/while1618/i18n-auto-translation.git"
  },
  "dependencies": {
    "@google-cloud/translate": "7.2.1",
    "axios": "1.4.0",
    "deep-object-diff": "1.1.9",
    "glob": "10.2.6",
    "html-entities": "2.3.3",
    "just-extend": "6.2.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.2.5",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.41.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}
